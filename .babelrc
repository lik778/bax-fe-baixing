{
  "presets": [
    ["env", {
      "targets": {
        "firefox": 59,
        "safari": 11,
        "chrome": 45,
        "edge": 16
      },
      "exclude": [
        "transform-regenerator"
      ]
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["module-resolver", {
      "root": [],
      "alias": {
        "constant": "./constant",
        "config": "./config",
        "com": "./component",
        "base": "./base",
        "util": "./util",
        "api": "./api"
      }
    }]
  ]
}
